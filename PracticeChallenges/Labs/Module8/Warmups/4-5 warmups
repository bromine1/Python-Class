#Fixed version:
try:
    import os

    n = 20

    for i in range(1, n + 1):
        error = os.strerror(i) 
        print("Error code % d:" % i, error)
except:
    print('initial error')

# 1. readlines will return an empty string
# 2 code is meant to read out the consonants in a file
# Needs indentation
# Should account for newline characters
for line in open("file", "rt"):
    for char in line:
        if char.lower() not in "aeiouy ":
            print(char, end='')